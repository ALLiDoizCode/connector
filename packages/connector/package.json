{
  "name": "@agent-runtime/connector",
  "version": "0.1.0",
  "description": "Connector with BTP support and tri-chain settlement",
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    }
  },
  "bin": {
    "agent-runtime": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "npm run build:ui && tsc",
    "build:ui": "cd explorer-ui && NODE_ENV=development npm install && npm run build",
    "build:connector-only": "tsc",
    "start": "node dist/main.js",
    "dev:ui": "cd explorer-ui && npm run dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:rfc-links": "jest test/integration/rfc-links.test.ts",
    "test:acceptance": "jest --config jest.acceptance.config.js --testPathPattern=acceptance",
    "test:load": "jest --config jest.load.config.js --testPathPattern=load-test",
    "test:security": "jest --testPathPattern=security --testTimeout=60000",
    "test:domain": "node scripts/run-tests.js --domain",
    "test:epic": "node scripts/run-tests.js --epic",
    "test:list": "node scripts/run-tests.js --list-domains",
    "test:list-epics": "node scripts/run-tests.js --list-epics",
    "test:unit": "jest --testPathIgnorePatterns='test/integration|test/acceptance'",
    "test:integration": "jest --testPathPattern=test/integration",
    "test:tigerbeetle": "jest --testPathPattern='tigerbeetle|account-manager|packet-forwarding-with-accounting'",
    "test:xrp": "jest --testPathPattern='xrp-|xrpl-|rippled-'",
    "test:aptos": "jest --testPathPattern='aptos-|connector-aptos-'",
    "test:settlement": "jest --testPathPattern='settlement-|claim-|multi-chain-|tri-chain-'",
    "test:explorer": "jest --testPathPattern='explorer-'",
    "test:btp": "jest --testPathPattern='btp-'",
    "test:docker": "jest --testPathPattern='docker-|deployment|mesh-topology|full-stack'",
    "lint": "eslint src --ext .ts",
    "dev": "tsc --watch",
    "benchmark": "ts-node scripts/benchmark.ts",
    "benchmark:toon": "tsx src/agent/toon-codec.benchmark.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.24",
    "@aptos-labs/ts-sdk": "^1.39.0",
    "@aws-sdk/client-kms": "^3.500.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@azure/identity": "^4.0.0",
    "@azure/keyvault-keys": "^4.8.0",
    "@google-cloud/kms": "^3.2.0",
    "@libsql/client": "0.14.0",
    "@agent-runtime/shared": "*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.0",
    "@opentelemetry/resources": "^1.25.0",
    "@opentelemetry/sdk-node": "^0.52.0",
    "@opentelemetry/semantic-conventions": "^1.25.0",
    "@toon-format/toon": "2.1.0",
    "@types/connect-timeout": "^1.9.0",
    "@types/cors": "^2.8.19",
    "@types/multer": "^2.0.0",
    "ai": "^4.3.19",
    "better-sqlite3": "^11.8.1",
    "bip39": "^3.1.0",
    "commander": "^12.1.0",
    "connect-timeout": "^1.9.1",
    "cors": "^2.8.6",
    "ethereum-cryptography": "^3.2.0",
    "ethers": "^6.16.0",
    "express": "4.18.x",
    "inquirer": "^9.3.8",
    "js-yaml": "^4.1.0",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "nostr-tools": "^2.20.0",
    "pino": "^8.21.0",
    "pino-abstract-transport": "^3.0.0",
    "prom-client": "^15.1.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.33.5",
    "tigerbeetle-node": "0.16.68",
    "tslib": "^2.8.1",
    "ws": "^8.16.0",
    "xrpl": "^2.14.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.19",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0"
  }
}
